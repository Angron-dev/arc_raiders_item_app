{
    "compilerOptions": {
        "target": "ES2020",
        "module": "ESNext",
        "jsx": "react-jsx",
        "moduleResolution": "Node",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "skipLibCheck": true,
        "allowJs": true,
        "baseUrl": "./resources/js",
        "paths": {
            "@/*": ["./*"]
        }
    },
    "include": ["resources/js/**/*"]
}
